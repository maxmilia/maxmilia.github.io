&lt;/span>&lt;span class="c1">&lt;/span> &lt;span class="nx">fmt&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nf">Println&lt;/span>&lt;span class="p">(&lt;/span>&lt;span class="nx">ids&lt;/span>&lt;span class="p">,&lt;/span> &lt;span class="nx">vp&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nx">WorkData&lt;/span>&lt;span class="p">,&lt;/span> &lt;span class="nx">wd&lt;/span>&lt;span class="p">)&lt;/span>
